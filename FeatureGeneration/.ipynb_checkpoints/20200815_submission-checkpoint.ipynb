    "    [transforms.Resize(size),transforms.ToTensor(),\n",
    "     transforms.Normalize((0.5, 0.5, 0.5), (0.5, 0.5, 0.5))])\n",
    "    \n",
    "    image_transformed=torch.empty(len(data),3,size[0],size[1])\n",
    "    \n",
    "    for image_counter in range(len(data)):  \n",
